{
  "name": "qyu",
  "description": "A general-purpose asynchronous job queue for Node.js",
  "version": "1.0.1",
  "license": "MIT",
  "author": "Dor Shtaif <dorshtaif@gmail.com>",
  "keywords": [
    "queue",
    "job",
    "promise",
    "async",
    "throttle",
    "async pool"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shtaif/qyu"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=7.6.0"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "test": "ts-mocha -p ./tsconfig-tests.json",
    "code-check": "prettier --check \"./src/**/*.{ts,js}\" && eslint ./src --cache && pnpm run test --type-check && tsc --noEmit",
    "build": "rm -rf ./dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && node ./scripts/mark-dist-dirs-package-module-type.js",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.17.12",
    "@types/sinon": "^10.0.16",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "mocha": "^9.2.2",
    "prettier": "^2.8.8",
    "sinon": "^14.0.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5"
  }
}
